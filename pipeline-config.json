{
  "pipeline": {
    "name": "DevOpsDemoPipeline",
    "roleArn": "arn:aws:iam::030901817847:role/codepipeline-devops-role",
    "artifactStore": {
      "type": "S3",
      "location": "devops-pipeline-artifacts-1765413404"
    },
    "stages": [
      {
        "name": "Source",
        "actions": [
          {
            "name": "SourceAction",
            "actionTypeId": {
              "category": "Source",
              "owner": "AWS",
              "provider": "S3",
              "version": "1"
            },
            "configuration": {
              "S3Bucket": "devops-pipeline-artifacts-1765413404",
              "S3ObjectKey": "source-code.zip"
            },
            "outputArtifacts": [
              {
                "name": "SourceOutput"
              }
            ]
          }
        ]
      },
      {
        "name": "Build",
        "actions": [
          {
            "name": "BuildAction",
            "actionTypeId": {
              "category": "Build",
              "owner": "AWS",
              "provider": "CodeBuild",
              "version": "1"
            },
            "configuration": {
              "ProjectName": "DevOpsDemoBuild"
            },
            "inputArtifacts": [
              {
                "name": "SourceOutput"
              }
            ],
            "outputArtifacts": [
              {
                "name": "BuildOutput"
              }
            ]
          }
        ]
      },
      {
        "name": "Deploy",
        "actions": [
          {
            "name": "DeployAction",
            "actionTypeId": {
              "category": "Deploy",
              "owner": "AWS",
              "provider": "CloudFormation",
              "version": "1"
            },
            "configuration": {
              "ActionMode": "CREATE_UPDATE",
              "StackName": "devops-demo-stack",
              "TemplatePath": "BuildOutput::packaged-template.yml",
              "Capabilities": "CAPABILITY_IAM",
              "RoleArn": "arn:aws:iam::030901817847:role/codepipeline-devops-role"
            },
            "inputArtifacts": [
              {
                "name": "BuildOutput"
              }
            ]
          }
        ]
      }
    ]
  }
}
